{
  "name": "616 Degrees of Separation",
  "short_name": "616 Degrees",
  "description": "Connect Marvel characters through comic book appearances in this daily puzzle game",
  "id": "com.crescender.616degrees",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "minimal-ui", "standalone", "browser"],
  "orientation": "portrait",
  "dir": "ltr",
  "lang": "en-AU",
  "theme_color": "#5B21B6",
  "background_color": "#1E1B4B",
  "categories": ["games", "entertainment", "puzzle"],
  "prefer_related_applications": false,
  "related_applications": [],
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "navigate-new"
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "scope_extensions": [
    {
      "origin": "*.vercel.app"
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "text/plain": [".txt"],
        "application/json": [".json"]
      }
    }
  ],
